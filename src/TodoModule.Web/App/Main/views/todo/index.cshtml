<div ng-controller="app.views.todo as vm">
    <h1>Todo page</h1>
    <div class="row">
        <div class="col-md-12">
            <ul class="list-group">
                <li class="list-group-item" ng-repeat="todo in vm.todos">
                    <h4>{{todo.text}}</h4>
                </li>
            </ul>
        </div>
    </div>
</div>